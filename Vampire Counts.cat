    <profile id="50c3-d69e-27fe-c4f6" name="Zombie Dragon" publicationId="10a5-5781-61f5-86ae" page="209" hidden="false" typeId="d9ed-a507-0102-d498" typeName="Model">
      <characteristics>
        <characteristic name="M" typeId="09ad-18df-607b-1215">6</characteristic>
        <characteristic name="WS" typeId="f049-8a65-9c04-1559">4</characteristic>
